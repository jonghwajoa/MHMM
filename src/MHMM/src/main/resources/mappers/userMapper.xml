<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="xyz.mhmm.mappers.userMapper">

	<select id="findExistByEmail" resultType="string">
		select email from
		tbl_user where email = #{email}
	</select>

	<insert id="create" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
		insert into tbl_user (name,email) values
		(#{name},
		#{email} )
	</insert>

</mapper>
